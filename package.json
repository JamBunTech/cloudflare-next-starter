{
  "name": "clouflare-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "cz",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "lefthook:install": "lefthook install",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.5.1",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cz-git": "^1.12.0",
    "lefthook": "^1.12.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "ultracite": "5.0.32",
    "wrangler": "^4.24.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
